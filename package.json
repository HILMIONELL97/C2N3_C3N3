    "description": "C'est un projet pour valider les comp√©tences C3N3 (C2N3).",
    "main": "index.js",
    "scripts": {
        "compile": "node-sass Starter-file/sass -o Starter-file/dist/css",
        "watch": "node-sass -w Starter-file/sass -o Starter-file/dist/css",
        "compress": "node-sass --output-style compressed Starter-file/sass -o Starter-file/dist/css",
        "toPrefixed": "postcss starter-file/dist/css/fichier.css --use=autoprefixer --map=false --output=starter-file/dist/css/fichier.css",
        "concat": "concat -o Starter-file/dist/css/style.css Starter-file/dist/css/fichier.css",
        "build": "npm run compile && npm run compress && npm run toPrefixed"
    },
    "browserslist": [
        "last 4 versions"
    ],
    "repository": {
